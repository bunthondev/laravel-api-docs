{
    "name": "puchan/laravel-api-docs",
    "description": "Automatic API documentation generator for Laravel applications",
    "keywords": [
        "laravel",
        "api",
        "documentation",
        "swagger",
        "openapi",
        "api-docs"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Puchan",
            "email": "puchan@example.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Puchan\\LaravelApiDocs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Puchan\\LaravelApiDocs\\ApiDocsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
